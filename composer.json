{
    "name": "neondigital/laravel-polygot",
    "description": "Generate language files automatically",
    "keywords": [
        "Neon Digital",
        "Laravel Locale",
        "Laravel Polygot",
    ],
    "homepage": "https://bitbucket.org/alecneon/laravel-polyglot/",
    "license": "MIT",
    "require": {
        "php" : ">=5.5.9",
        "guzzlehttp/guzzle": "~5.3"
    },
    "require-dev": {
        "laravel/framework": "5.2.*",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Neondigital\\LaravelPolyglot\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Neondigital\\LaravelPolyglot\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
